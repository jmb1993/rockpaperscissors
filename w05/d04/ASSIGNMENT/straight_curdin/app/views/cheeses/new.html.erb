<%= form_for @cheese do |f| %>
<%= f.label :name%>
<%= f.text_field :name %>

<%= f.label :milk_type %>
<%= f.text_field :milk_type %>

<%= f.label :description %>
<%= f.text_field :description %>

<%= f.label :image_url %>
<%= f.text_field :image_url %>
<%= f.submit 'CREATE A DANK NEW CHEESE' %>
<% end %>
